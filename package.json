{
  "name": "solana-dex-sdk",
  "version": "1.0.5",
  "description": "A simple SDK to interact with Raydium and Solana DEXs",
  "main": "dist/cjs/index.js",  
  "module": "dist/esm/index.js", 
  "types": "dist/cjs/index.d.ts",  
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"   
    }
  },
  "scripts": {
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs", 
    "build:esm": "tsc --module ESNext --outDir dist/esm",  
    "build": "npm run build:cjs && npm run build:esm", 
    "test": "jest",
    "example": "node dist/examples/basicSwap.js",
    "prepublish": "npm run build",
    "start": "ts-node src/index.ts"
  },
  "dependencies": {
    "@project-serum/anchor": "^0.26.0",
    "@project-serum/serum": "^0.13.65",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
    "@raydium-io/raydium-sdk-v2": "^0.1.73-alpha",
    "@solana/spl-token": "^0.3.11",
    "@solana/web3.js": "^1.95.4",
    "axios": "^1.7.7",
    "base58-js": "^2.0.0",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "@types/bn.js": "^5.1.6",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.19.74",
    "jest": "^29.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "solana",
    "raydium",
    "swap",
    "dex",
    "defi"
  ],
  "author": "Cristian Machado",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmacha2/solana-dex-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/cmacha2/solana-dex-sdk/issues"
  },
  "homepage": "https://github.com/cmacha2/solana-dex-sdk#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*"
  ]
}
